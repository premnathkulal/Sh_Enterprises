<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sh_enterprises">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="sh_enterprises"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Detailes" />
        <activity android:name=".categories" />
        <activity
            android:name=".Home_page"
            android:label="Home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Notes_List" />
        <activity android:name=".cart_data" />
        <activity android:name=".Login" />
        <activity android:name=".retailer_list" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>